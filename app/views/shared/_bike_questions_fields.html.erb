<%= f.label :make, 'Make' %>
<%= f.collection_select :make, Bike.unique_by_make, :id , :make %>

<%= f.label :model, 'Model' %>
<%= f.collection_select :model, Bike.unique_by_model.reorder("model ASC"), :id , :model %>

<%= f.label :year_manufactured, 'Year' %>
<%= f.date_field :year_manufactured %>
